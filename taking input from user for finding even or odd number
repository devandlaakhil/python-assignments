def count(lst):
    even = 0
    odd = 0
    for i in lst:
        if i % 2 ==0:
            even+=1
        else:
            odd+=1
    return even,odd
l=int(input("enter the length"))
lst=[]
for m in range(l):
    elements=int(input("enter the values"))
    lst.append(elements)
even,odd=count(lst)
print("even:{} and odd:{}".format(even,odd))
